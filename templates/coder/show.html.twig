{% extends 'base.html.twig' %}

{% block title %}Coder{% endblock %}

{% block body %}
    <h1>Coder</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ coder.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ coder.name }}</td>
            </tr>
            <tr>
                <th>Surname</th>
                <td>{{ coder.surname }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ coder.description }}</td>
            </tr>
            <tr>
                <th>Url</th>
                <td>{{ coder.url }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_coder_index') }}">back to list</a>

    <a href="{{ path('app_coder_edit', {'id': coder.id}) }}">edit</a>

    {{ include('coder/_delete_form.html.twig') }}
